{
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
           "mainClass": "${file}",
            "stepFilters": {
                "classNameFilters": [
                    "org.springframework.*",
                    "$JDK",
                    "org.junit.*",
                    "java.*",
                    "javax.*",
                    "com.sun.*",
                    "sun.*",
                    "sunw.*",
                    "org.omg.*",
                    "java.lang.ClassLoader",
                    "Class.class"
                    
                    
                ],
                "skipSynthetics": false,
                "skipStaticInitializers": false,
                "skipConstructors": false
            }
        }
    ]
}
